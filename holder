*/

        }
      </Grid>
      {
        new Date(data[0].schedule.dates[0].games[activeStep].gameDate).toLocaleTimeString()
      }
      <Grid container display="flex" justifyContent="space-evenly">
        <MobileStepper
          variant="progress"
          steps={data[0].schedule.dates[0].games.length}
          position="static"
          activeStep={activeStep}
          sx={{ maxWidth: 400, flexGrow: 1 }}
          nextButton={
            <Button
              size="small"
              onClick={handleNext}
              disabled={activeStep === data[0].schedule.dates[0].games.length - 1}
            >
              Next
              {theme.direction === 'rtl' ? (
                <KeyboardArrowLeft />
              ) : (
                <KeyboardArrowRight />
              )}
            </Button>
          }
          backButton={
            <Button size="small" onClick={handleBack} disabled={activeStep === 0}>
              {theme.direction === 'rtl' ? (
                <KeyboardArrowRight />
              ) : (
                <KeyboardArrowLeft />
              )}
              Back
            </Button>
          }
        />
      </Grid>
    </Paper>